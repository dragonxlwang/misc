"*":
  "exception-reporting":
    userId: "37115345-b4cc-8e90-d39d-02aef4fc2d04"
  welcome:
    showOnStartup: false
  core:
    disabledPackages: [
      "selection-highlight"
      "indent-guide-improved"
      "block-cursor"
      "git-grep"
      "nuclide-file-tree"
      "nuclide-remote-projects"
      "remote-edit"
      "formatter"
      "linter-pep8"
      "vim-mode"
    ]
    themes: [
      "one-dark-ui"
      "brahalla-syntax"
    ]
    packagesWithKeymapsDisabled: [
      "compare-files"
    ]
  editor:
    invisibles: {}
    fontSize: 11
    fontFamily: "monaco"
    showIndentGuide: true
    autoIndentOnPaste: false
  minimap:
    plugins:
      "find-and-replace": true
      "git-diff": true
      "highlight-selected": true
      selection: true
  "remote-edit": {}
  "remote-sync": {}
  nuclide:
    remoteProjectsConfig: []
  linter:
    errorPanelHeight: 46
    showErrorTabLine: true
  "linter-clang": {}
  "selection-highlight":
    lightTheme: true
    highlightBackground: true
  "highlight-selected":
    lightTheme: true
    highlightBackground: true
    ignoreCase: true
  "block-cursor":
    global:
      blinkOff: {}
      blinkOn:
        backgroundColor:
          red: 42
          green: 37
          blue: 17
          alpha: 1
        borderColor:
          red: 249
          green: 105
          blue: 76
          alpha: 1
      cursorLineFix: true
  "autocomplete-plus": {}
  "one-dark-ui":
    layoutMode: "Compact"
  "autocomplete-python":
    pythonPath: "/usr/local/bin/python"
    useSnippets: "all"
    outputDebug: true
  "linter-python-pep8":
    pep8DirToExecutable: "/usr/local/bin/"
    ignoreErrorCodes: "W292"
  "linter-pep8":
    maxLineLength: 80
    pep8ExecutablePath: "/usr/local/bin/pep8"
    ignoreErrorCodes: [
      "E302"
    ]
  "advanced-open-file":
    helmDirSwitch: true
  "tree-view-git-status": {}
  "git-history": {}
  "git-log": {}
  "git-plus": {}
  "git-projects": {}
  "git-status": {}
  "git-diff":
    showIconsInEditorGutter: true
  "save-session":
    dataSaveFolder: "/Users/xiaolong/.atom/packages/save-session/projects"
  "latex-plus":
    bibtexEnabled: true
    texBin: "/Library/TeX/texbin/tex"
    texInputs: "/Users/xiaolong/texmf"
  "linter-chktex":
    executablePath: "/Library/TeX/texbin/chktex"
  "autocomplete-bibtex":
    bibtex: [
      "/Library/TeX/texbin/bibtex"
    ]
  latexer: {}
  latex:
    openResultAfterBuild: false
  "pdf-view": {}
  "merge-conflicts": {}
  "atom-beautify": {}
  "atom-terminal": {}
  "python-yapf":
    yapfPath: "/usr/local/bin/yapf"
    yapfStyle: "{based_on_style: google, column_limit: 80}"
    formatOnSave: true
  "python-autopep8":
    autopep8Path: "/usr/local/bin/autopep8"
    formatOnSave: true
    maxLineLength: 80
  "linter-flake8":
    executablePath: "/usr/local/bin/flake8"
    maxLineLength: 80
    ignoreErrorCodes: [
      "E302"
      "D100"
      "D101"
      "D102"
      "D103"
      "D104"
      "D105"
      "D200"
      "D201"
      "D202"
      "D203"
      "D204"
      "D205"
      "D208"
      "D209"
      "D300"
      "D400"
      "D401"
      "D402"
      "E123"
      "E226"
      "E241"
      "N802"
      "N803"
      "N806"
      "N812"
    ]
  "formatter-clang-format": {}
  "clang-format":
    executable: "/usr/local/bin/clang-format"
    style: "Google"
    formatCPlusPlusOnSave: false
    formatCOnSave: false
  "python-tools":
    pythonPath: "/usr/local/bin/python"
  "spell-check":
    grammars: [
      "text.tex.latex"
    ]
  "vim-mode": {}
